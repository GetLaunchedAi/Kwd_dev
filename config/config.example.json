{
  "clickup": {
    "apiToken": "env:CLICKUP_API_TOKEN",
    "webhookSecret": "env:CLICKUP_WEBHOOK_SECRET",
    "triggerStatus": "Ready to Code",
    "completionStatuses": ["complete", "completed", "done", "closed", "cancelled"],
    "filters": {
      "excludeStatuses": [],
      "includeOnlyStatuses": null
    }
  },
  "git": {
    "clientWebsitesDir": "./client-websites",
    "githubToken": "env:GITHUB_TOKEN",
    "defaultBranch": "main",
    "folderMapping": {}
  },
  "cursor": {
    "cliPath": "cursor",
    "autoOpen": true,
    "agentMode": true,
    "agentTriggerMethod": "api"
  },
  "testing": {
    "timeout": 300000
  },
  "server": {
    "port": 3000
  },
  "approval": {
    "method": "email",
    "email": {
      "smtp": {
        "host": "env:SMTP_HOST",
        "port": 587,
        "secure": false,
        "auth": {
          "user": "env:SMTP_USER",
          "pass": "env:SMTP_PASS"
        }
      },
      "from": "env:EMAIL_FROM",
      "approvalUrl": "http://localhost:3000/approve/{token}"
    },
    "slack": {
      "webhookUrl": "env:SLACK_WEBHOOK_URL"
    }
  }
}















